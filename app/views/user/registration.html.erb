<h1>It's take few second to Register here </h1>

  <% form_for :user do |f| %>
    
    <fieldset>
      <legend><h3><i><div id="asd">Enter Your Details</div></i></h3></legend>
        <%= error_messages_for "user" %>
       <table width="400px">
      
        <tr>
          
          	<td width="80"><label for="first_name">First Name</label></td>
          	<td width="4"><strong>:</strong></td>
          	<td width="6"><%= f.text_field :first_name %><td width="107">
          
        </tr>

        <tr>
          
            <td><label for="last_name">Last Name</label></td>
            <td width="4"><strong>:</strong></td>
            <td><%= f.text_field :last_name %></td>
          
        </tr>
        
        <tr>
          
            <td><label for="user_id">User Id</label></td>
            <td width="4"><strong>:</strong></td>
            <td><%= f.text_field :user_id %></td>
          
        </tr>
        
        <tr>
          
            <td><label for="password">Password</label></td>
            <td width="4"><strong>:</strong></td>
            <td><%= f.password_field :password,
                                  :size=>'30px',
                                  :maxlength=>User::PASSWORD_MAX_LENGTH %>
          </td>
          
        </tr>
        
        <tr>
          
            <td><label for="email">Email</label></td>
            <td width="4"><strong>:</strong></td>
            <td><%= f.text_field :email %></td>
          
        </tr>
        
        <tr>
        
            <td><label for="phone">Phone</label></td>
            <td width="4"><strong>:</strong></td>
            <td><%= f.text_field :phone %></td>
          
        </tr>
        
        <tr>
        	<td><p class="links"></p></td>
        	<td width="4"></td>
        	<td><%= submit_tag "Register", :class => "submit" %></td>
        </tr>
        
        </table>
        
      </fieldset>
<% end %>